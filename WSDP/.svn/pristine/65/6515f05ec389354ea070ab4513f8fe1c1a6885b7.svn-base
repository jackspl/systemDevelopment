}
.specificUser .el-row {
  position: initial;
}



.specificUser .content{
  margin-bottom: 10px;
}
.specificUser .seach {
  margin-left: 80px;
}
.specificUser .el-input--suffix .el-input__inner {
  padding-right: 0px;
}
.specificUser .el-pagination {
  float: right;
  padding: 0px;
}
.specificUser .el-tabs--border-card {
  border: 0px;
}

.specificUser .el-form--inline .el-form-item {
  margin-right: 0px;
  display: flex;
}
.specificUser .el-form--inline .el-form-item__content {
  flex: 1;
}
.specificUser .el-autocomplete {
  flex: 1;
}
.specificUser .el-form--inline .el-form-item__label {
  white-space: nowrap;
}
/* 搜索部分 */
.specificUser .queryBox {
  width: 100%;
  height: 74px;
  background-color: #02356f;
  border: none;
}
.specificUser .el-form-item {
  float: left;
  letter-spacing:2px;
}
.specificUser .el-form-item__label {
  /* width: 80px; */
  padding-right: 5px;
  box-sizing: border-box;
  color: white;
  font-size: 16px;
  text-align: right;
  vertical-align: middle;
  float: left;
  line-height: 40px;
}
.el-table--border,
.el-table:before {
  border-color: #00639e;
  border: 1px solid #00639e;
  border-radius: 5px;
}
.specificUser .el-input__inner {
  background-color: #083760;
  color: white;
  border: 1px solid #00639e;
}
.specificUser .el-input__prefix {
  color: #00cbd5;
}
.specificUser input::-webkit-input-placeholder {
  color: white;
}
.specificUser input::-moz-input-placeholder {
  color: white;
}
.specificUser input::-ms-input-placeholder {
  color: white;
}
.specificUser .el-button--primary {
  letter-spacing:2px;
  box-sizing: border-box;
  background-color: #3284c3;
}
.specificUser .el-button+.el-button {
  letter-spacing:2px;
  box-sizing: border-box;
  background-color: #bfbfbf;
  color: black;
  font-weight: 400;
  
}
.specificUser .el-tabs--border-card>.el-tabs__header {
  border-bottom: 0px;
}
/* echarts部分 */
.specificUser .histogramBox {
  /* width: 100%; */
  overflow: hidden;
  height: 430px;
  border: 1px solid #2f69a0;
  border-radius: 5px;
  margin-bottom: 20px;
}
/* Excel表格部分 */
.el-table--border:after,
.el-table--group:after,
.el-table:before {
    background-color: #2f69a0;
    border-radius: 5px;
}

.el-table--border,
.el-table--group {
    border-color: #2f69a0;
    border-radius: 5px;
}

.el-table td,
.el-table th.is-leaf {
border-bottom: 1px solid #8094ad;
}

.specificUser .el-table--border th,
.el-table--border th.gutter:last-of-type {
        border-bottom: 1px solid #344e77;
}

.el-table--border td{
  border-right: 1px solid #344e77;
}
.el-table--border th {
border-right: 1px solid #344e77;
}
.right .el-tabs--border-card>.el-tabs__content {
  background-color: #02356f;
  overflow: none;
}
.el-pagination__total {
  color: white;
}
.el-pagination button:disabled {
  background-color: #02356f;
  color: white;
}
.el-pager, .el-pager li {
  background-color: #02356f;
  color: white;
}
.el-pagination .btn-next, .el-pagination .btn-prev {
  background-color: #02356f;
  color: white;
}

.el-table tbody tr:hover>td {
  background-color: #385c95 !important;
  color: #13eaf2 !important;
}
/* 分页背景色 */
.el-input.is-disabled .el-input__inner {
  background-color: #02356f;

}
/* 多出来的bug */
.el-tabs__nav-wrap::after {
  position: fixed;
  width: 0px;
  height: 0px;
  border: 0px;
}
.specificUser .right .el-tabs__nav {
  margin-bottom: 0px;
}
.specificUser .el-tabs__nav-scroll {
  overflow: initial;
}
.specificUser .el-tabs__header {
  margin: 0;
  display: none;
}

.specificUser .el-input.is-disabled .el-input__inner {
  background-color: #083760;
  color: white;
  border: 1px solid #00639e;
  margin-left: 5px;
}
/* 表格高度40px */
.specificUser .el-table__header tr,
  .el-table__header th {
    padding: 0;
    height: 40px;
}
.specificUser .el-table__body tr,
  .el-table__body td {
    padding: 0;
    height: 40px;
}
/* 表格高亮过渡效果 */
.specificUser .el-table--enable-row-transition .el-table__body td {
    -webkit-transition: background-color 0s ease;
    transition: background-color 0s ease;
}
