  padding-right: 30px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.scenMachine .seachAll .searchImg{
  float: left;
  width: 38px;
  height: 50px;
  padding-top: 5px;
  margin-right: 10px;
  box-sizing: border-box;
}
.scenMachine .seachAll .searchUserName{
  float: left;
  width: 238px;
  height: 50px;
  margin-right: 10px;
  box-sizing: border-box;
}
.scenMachine .seachAll .searchUserName .el-input--suffix .el-input__inner {
  width: 154px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.scenMachine .seachAll .searchScenarioName{
  float: left;
  width: 324px;
  height: 50px;
  margin-right: 10px;
  box-sizing: border-box;
}
.scenMachine .seachAll .searchScenarioName .el-input--suffix .el-input__inner {
  width: 234px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.scenMachine .seachAll .searchStarTime{
  float: left;
  width: 274px;
  height: 50px;
  margin-right: 10px;
  box-sizing: border-box;
}
.scenMachine .seachAll .searchEndTime{
  float: left;
  width: 274px;
  height: 50px;
  margin-right: 10px;
  box-sizing: border-box;
}
.scenMachine .seachAll .searchButton{
  float: right;
  width: 160px;
  height: 50px;
  box-sizing: border-box;
}
.scenMachine .seachAll .searchButton .el-button+.el-button {
  width: 74px;
  margin-left: 10px;
}
.scenMachine #basedClient {
  height: 430px;
}
.scenMachine .el-col-4 {
  width: 15.66667%;
  border: 1px solid #00639e;
  margin-right: 10px;
  border-radius: 5px;
  background-color: #164276;
}

.scenMachine .el-input {
  width: auto;
}
.scenMachine .tableHeader  .el-input__inner {
  background-color: rgb(54, 90, 136);
  color: white;
  border: 0px solid rgb(54, 90, 136);
}
/* 总信息 */
.scenMachine .infoSum {
  width: 100%;
   height: 50px;
  padding-top: 14px;
  background-color: #164276;
  border: 1px solid #00639e;
  border-radius: 4px;
  text-align: center;
  font-size: 16px;
  color: white;
  margin-bottom: 10px;
  box-sizing: border-box;
  float: left;
}
.scenMachine .infoSum .infoTimeSum {
  margin-right: 20px;
}
.scenMachine .infoSum .infoNumSum {
  margin-right: 20px;
}
.scenMachine .infoSum .infoAvgTimeSum {
  margin-right: 20px;
}
.scenMachine .infoSum .infoUserSum {
  margin-right: 20px;
}
.el-input__inner {
  -webkit-transition: border-color 0s cubic-bezier(.645,.045,.355,1);
  transition: border-color 0s cubic-bezier(.645,.045,.355,1);
}

/* ------------------------------------------- */
.scenMachine .el-form-item__label {
  padding-right: 5px;
}
/* 搜索框公共样式 */
.scenMachine .el-input__inner {
  height: 30px;
  line-height: 30px;
}
.scenMachine .el-button {
  border: 0px;
  padding: 8px 20px;
  text-align: center;
  box-sizing: border-box;
}
.scenMachine .el-form-item__content {
  float: left;
}
/* ---------------------------------------------- */
