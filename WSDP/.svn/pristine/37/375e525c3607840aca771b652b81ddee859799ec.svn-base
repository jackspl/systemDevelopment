/* 清除浮动的公共类 */
.clearfix:before,
.clearfix:after { 
content:"";
display:table;
}
.clearfix:after {
clear:both;
}
.scenMachine .main {
  padding-bottom: 30px;
  text-align: center;
}
.scenMachine .el-table .warning-row  {
  background: rgba(229, 236, 175, 0.38);
}
.scenMachine .el-button.is-round {
  padding: 3px 7px;
}
.scenMachine .excel {
  float: right;
  right: 20px;
  padding: 3px 7px;
}
.scenMachine .el-row {
  position: initial;
}
.scenMachine .form { 
  padding-top: 10px;
  white-space: nowrap;
}
.scenMachine .el-pagination {
  float: right;
  padding-top: 10px;
}

.scenMachine .el-form--inline .el-form-item {
  display: flex;
}
.scenMachine .el-form--inline .el-form-item__content {
  flex: 1;
}
.scenMachine .el-autocomplete {
  flex: 1;
}
.scenMachine .el-form--inline .el-form-item__label {
  white-space: nowrap;
}
.scenMachine .el-input--suffix .el-input__inner {
  padding-right: 0px;
}
/* 搜索部分 */
.scenMachine .queryBox {
  width: 100%;
  height: 80px;
  background-color: #02356f;
  border: none;
}
.scenMachine .el-form-item {
  float: left;
  letter-spacing:2px;
}

.scenMachine .el-form-item__label {
  color: white;
  font-size: 16px;
  text-align: right;
  vertical-align: middle;
  float: left;
  line-height: 40px;
}
.scenMachine .el-input__inner {
  background-color: #083760;
  color: white;
  border: 1px solid #00639e;
}
.scenMachine .el-input__prefix {
  color: #00cbd5;
}
.scenMachine input::-webkit-input-placeholder {
  color: white;
}
.scenMachine input::-moz-input-placeholder {
  color: white;
}
.scenMachine input::-ms-input-placeholder {
  color: white;
}
.scenMachine .el-button--primary {
  letter-spacing:2px;
  box-sizing: border-box;
  background-color: #3284c3;
}
.scenMachine .el-button+.el-button {
  letter-spacing:2px;
  box-sizing: border-box;
  background-color: #bfbfbf;
  color: black;
  font-weight: 400;
  
}
.scenMachine .el-tabs--border-card>.el-tabs__header {
  border-bottom: 0px;
}
/* echarts部分 */
.scenMachine .linePie {
  float: left;
  width: 100%;
  height: 430px;
  border: 1px solid #2f69a0;
  border-radius: 5px;
  margin-bottom: 20px;
}
/* Excel表格部分 */
.el-table--border:after,
.el-table--group:after,
.el-table:before {
    background-color: #2f69a0;
    border-radius: 5px;
}

.el-table--border,
.el-table--group {
    border-color: #2f69a0;
    border-radius: 5px;
}

.el-table td,
.el-table th.is-leaf {
border-bottom: 1px solid #8094ad;
}

.el-table--border th,
.el-table--border th.gutter:last-of-type {
        border-bottom: 1px solid #344e77;
}

.el-table--border td{
  border-right: 1px solid #344e77;
}
.el-table--border th {
border-right: 1px solid #344e77;
}
.right .el-tabs--border-card>.el-tabs__content {
  background-color: #02356f;
  overflow: none;
}
.el-pagination__total {
  color: white;
}
.el-pagination button:disabled {
  background-color: #02356f;
  color: white;
}
.el-pager, .el-pager li {
  background-color: #02356f;
  color: white;
}
.el-pagination .btn-next, .el-pagination .btn-prev {
  background-color: #02356f;
  color: white;
}

.el-table tbody tr:hover>td {
  background-color: #385c95 !important;
  color: #13eaf2 !important;
}
/* 分页背景色 */
.el-input.is-disabled .el-input__inner {
  background-color: #02356f;

}
.el-pagination.is-background .btn-next, .el-pagination.is-background .btn-prev, .el-pagination.is-background .el-pager li {
  background-color: #02356f;
}
.scenMachine .el-input.is-disabled .el-input__inner {
  background-color: #083760;
  color: white;
  border: 1px solid #00639e;
  margin-left: 5px;
}
/* 表格高度40px */
.scenMachine .el-table__header tr,
  .el-table__header th {
    padding: 0;
    height: 40px;
}
.scenMachine .el-table__body tr,
  .el-table__body td {
    padding: 0;
    height: 40px;
}
/* 表格高亮过渡效果 */
.scenMachine .el-table--enable-row-transition .el-table__body td {
    -webkit-transition: background-color 0s ease;
    transition: background-color 0s ease;
}
/* -------------------------------------- */
/* 改版后的 */
.scenMachine .seachAll {
  width: 100%;
  /* height: 70px; */
  /* padding: 10px 0; */
  box-sizing: border-box;
}
.scenMachine .seachAll .searchCriteria{
  float: left;
  width: 270px;
  height: 50px;
  box-sizing: border-box;
  margin-right: 10px;
}
