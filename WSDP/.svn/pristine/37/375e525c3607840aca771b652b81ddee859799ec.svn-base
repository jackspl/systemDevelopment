.scenMachine .seachSum {
  width: 100%;
  height: 40px;
  line-height: 40px;
  float: left;
  margin-top: 8px;
  margin-bottom: 10px;
}
/* 搜索条件 */
.scenMachine .seachSum .seachCond {
  /* width: 100px; */
  height: 40px;
  line-height: 40px;
  margin-right: 10px;
  float: left;
}
/* 图片 */
.scenMachine .seachSum .seachImg {
  height: 40px;
  line-height: 40px;
  margin-right: 10px;
  float: left;
}
.scenMachine .seachSum .seachImg .img {
  margin-top: 3px;
}
/* 主机名 */
.scenMachine .seachSum .hostName {
  height: 40px;
  line-height: 40px;
  margin-right: 10px;
  float: left;
}
/* 场景名称 */
.scenMachine .seachSum .sceneName {
  height: 40px;
  line-height: 40px;
  margin-right: 10px;
  float: left;
}
/* 开始时间 */
.scenMachine .seachSum .startTime {
  height: 40px;
  line-height: 40px;
  margin-right: 10px;
  float: right;
}
/* 结束时间 */
.scenMachine .seachSum .endTime {
  height: 40px;
  line-height: 40px;
  margin-right: 10px;
  float: right;
}
/* 搜索按钮 */
.scenMachine .seachSum .seachButton {
  height: 40px;
  line-height: 40px;
  margin-right: 6px;
  float: right;
}
.scenMachine .el-pagination span:not([class*=suffix]) {
  height: 30px;
  line-height: 30px;
}