/* 分页样式 */
.kpiRank .pagFen {
    float: right;
    z-index: 2;
    position: absolute;
    right: 0px;
    bottom: 3px;
    z-index: 2;
}

.kpiRank .pagFen.el-pagination span:not([class*=suffix]),
.el-pagination button, .el-pagination span:not([class*=suffix]),
.el-pager li {
    height: 22px;
    line-height: 22px;
}
.kpiRank .el-pagination button:disabled {
    background-color: #1d4b7f;
}
.kpiRank .el-pager li, .el-pagination .btn-prev, .el-pagination .btn-next {
