/* 清除浮动的公共类 */
.clearfix:before,
.clearfix:after {
    content: "";
    display: table;
}

.clearfix:after {
    clear: both;
}

/* 为了兼容IE6,7 */
.clearfix {
    *zoom: 1;
}

.kpiRank .el-tabs__content {
    overflow: inherit;
}

/* KPI顶部样式 */
/* 点击天周月改变背景色 */
.kpiRank .el-tabs__item.is-active {
    background-color: #596c7e;
}

/* kpi上下边框颜色 */
.kpiRank .el-tabs--card>.el-tabs__header .el-tabs__nav {
    border: 1px solid #3279a9;
}

/* kpi左右边框颜色 */
.kpiRank .el-tabs--card>.el-tabs__header .el-tabs__item {
    border-left: 1px solid #3279a9;
}

/* kpi点击底边框为零 */
.kpiRank .el-tabs--card>.el-tabs__header .el-tabs__item {
    border-bottom: 0px;
}

/* kpi默认颜色 */
.kpiRank .el-tabs__item {
    color: white;
}

/* kpi点击后颜色 */
.kpiRank #tab-seven {
    color: white;
}

/* 表单下边距清零 */
