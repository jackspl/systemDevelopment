/* 表示总长度背景色 */
.progress::-webkit-progress-bar {
    background-color: #f2f2f2;
    border-radius: 0.2rem;
}

/* 表示已完成进度背景色 */
.progress::-webkit-progress-value {
    background: #a21211;
    border-radius: 0.2rem;
}

