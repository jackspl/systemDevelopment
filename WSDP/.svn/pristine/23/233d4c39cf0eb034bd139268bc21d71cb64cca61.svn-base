.seachF3 .switchSum {
    width: 100%;
    padding: 10px 10px;
}
.seachF3 .switchSum .acrossDepDown {
    width: 74px;
    height: 20px;
    line-height: 20px;
    float: left;
    margin-right: 50px;
}
.seachF3 .switchSum .acrossDepDown .el-switch__label {
    color: white;
    position: absolute;
    display: block;
    width: 120px;
    left: 50px;
}
.seachF3 .switchSum .acrossDepDown .el-switch.is-checked .el-switch__core::after {
    margin-left: -9px;
    margin-top: -2px;
}
.seachF3 .switchSum .acrossDepDown .el-switch__core {
    height: 10px;
}
.seachF3 .switchSum .acrossDepDown .el-switch__core:after {
    content: "";
    position: absolute;
    border-radius: 100%;
    -webkit-transition: all .3s;
    transition: all .3s;
    width: 10px;
    height: 10px;
    margin-top: -1px;
    background-color: #FFF;
    margin-top: -2px;
    margin-left: 0px;
}
